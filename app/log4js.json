{"appenders": [
    {
        "type": "console",
        "layout": {
            "type": "pattern",
            "pattern": "%m"
        },
        "category": "console"
    },{
        "category": "file-appender",
        "type": "file",
        "filename": "logs/rest-service.log",
        "maxLogSize": 10240,
        "backups": 3,
        "layout": {
            "type": "pattern",
            "pattern": "%d{dd/MM hh:mm} %-5p %m"
        }
    }
],
"replaceConsole": true }
